GET => `SELECT * FROM games`
POST => `INSERT INTO games (name) VALUES (${name})`;
PATCH => `UPDATE games SET status = '${status}' WHERE id = ${id}`;
DELETE => `DELETE FROM games WHERE id = ${id}`


CREATE ROLE root WITH LOGIN PASSWORD 'root';
CREATE TABLE games 
(
  ID SERIAL PRIMARY KEY, 
  name VARCHAR(30) NOT NULL, 
  status VARCHAR(30) DEFAULT 'progress'
);

DROP table games;